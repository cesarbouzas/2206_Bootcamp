package principal;
import clases_Objeto.Coche;

public class Principal {

	public static void main(String[] args) {
	
		Coche c1=new Coche("Nissan","Xtrial");
		for(int i=0;i<=300;i++) {
		
			c1.decidirAcelerarFrenar();
			if(i%20==0&&i==0) {System.out.println();}
			System.out.print(c1.getTacometro()+",");
		}
		
		System.out.println( "media de :"+6*c1.getTacometro()/1000+" km/h");

}
}
